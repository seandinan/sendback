{
  "name": "sendback",
  "version": "0.0.3",
  "description": "A simple utility for formatting HTTP responses in an Express/Node application",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rm -rf dist",
    "build:src": "babel src --out-dir dist --copy-files --ignore \"src/**/*.test.js\"",
    "build": "npm run test && npm run clean:dist && npm run build:src",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [ "node", "express", "expressjs", "http" ],
  "author": "Sean Dinan <seancdinan@gmail.com> (seandinan.com)",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "jest": {
    "moduleFileExtensions": [ "js", "json" ],
    "transform": { "^.+\\.js$": "babel-jest" },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "webpack": "2.4.1",
    "webpack-cli": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seancdinan/sendback.git"
  },
  "bugs": {
    "url": "https://github.com/seancdinan/sendback/issues"
  },
  "homepage": "https://github.com/seancdinan/sendback#readme"
}
